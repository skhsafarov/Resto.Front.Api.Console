    <summary>Console logger for Resto Front API plugins using Named Pipes communication</summary>
    <releaseNotes>Initial release with Named Pipes logging support</releaseNotes>
    <copyright>Copyright ï¿½ 2025</copyright>
    <tags>resto console logging namedpipes iiko debugging plugin</tags>
    <dependencies>
      <group targetFramework=".NETFramework4.7.2" />
    </dependencies>
  </metadata>
  <files>
    <!-- Executable -->
    <file src="bin\Release\Resto.Front.Console.exe" target="tools" />
    <file src="bin\Release\Resto.Front.Console.exe.config" target="tools" />
    
    <!-- Optional: Include PDB for debugging -->
